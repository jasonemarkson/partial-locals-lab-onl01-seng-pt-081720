<%= form_tag students_path, :method => "get" do %>
    <%= label_tag(:search, "Find a student: ") %>
    <%= text_field_tag :search, params[:search] %>
    <ol>
        <li>
        <% Student.all.find do |s| %>
        <%= s.name %>
        <% end %>
        </li>
    </ol>
    <%= submit_tag("Search", :name => nil) %>
<% end %>

